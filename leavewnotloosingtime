queueonteleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/Absu245/saafyto/main/leavewnotloosingtime'))()")
bind = game.Players.LocalPlayer:GetMouse()
bind.KeyDown:Connect(function(keybind)
	if keybind == (_G.keybind) then
if _G.imready then return end
_G.imready = true
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,999999999999999,0)
		local LogService = game:GetService("LogService")
LogService.MessageOut:Connect(function(Message: string, MessageType: Enum.MessageType)
    if MessageType == Enum.MessageType.MessageError and Message:match("^Server Kick Message:") then
       		local ts = game:GetService("TeleportService")

		local p = game:GetService("Players").LocalPlayer

			ts:TeleportToPlaceInstance(game.PlaceId, game.JobId, p)
    end
end)
	end
	end)
	warn('loaded')
